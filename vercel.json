{
  "builds": [
    {
      "src": "keyboard_question/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "keyboard_question/wsgi.py"
    }
  ],
  "build": {
    "env": {
      "PYTHON_VERSION": "3.13"
    }
  }
}